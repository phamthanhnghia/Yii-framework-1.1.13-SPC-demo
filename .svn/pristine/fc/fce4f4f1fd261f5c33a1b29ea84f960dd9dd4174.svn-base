--
-- Table structure for table `gas_murad_product`
--

CREATE TABLE IF NOT EXISTS `gas_murad_product` (
  `id` mediumint(6) unsigned NOT NULL AUTO_INCREMENT,
  `code_real` varchar(100) DEFAULT NULL,
  `category_id` smallint(3) unsigned DEFAULT NULL COMMENT 'primary category',
  `category_type` tinyint(1) DEFAULT NULL COMMENT 'category type: Mụn, Nám & lão hóa, Cellulite, Vấn đề da khác',
  `category_id_2` smallint(3) unsigned DEFAULT NULL COMMENT 'category 2 cua product',
  `featured` tinyint(1) NOT NULL DEFAULT '0',
  `name` text,
  `status` tinyint(1) unsigned DEFAULT NULL,
  `type` tinyint(1) unsigned DEFAULT NULL COMMENT '1: Sữa rửa mặt, 2: Điều trị, 3: Kem dưỡng, chống nắng',
  `price_retail` decimal(16,2) DEFAULT NULL,
  `price_discount` decimal(10,0) NOT NULL,
  `unit` text COMMENT 'đơn vị:chai, lọ, cái',
  `unit_use` text COMMENT 'đơn vị sử dụng: l, ml, gram...',
  `size` text COMMENT '1 chai bao nhiêu ml:vd 200ml',
  `size_en` text COMMENT 'size nuoc ngoai',
  `name_vi` varchar(255) DEFAULT NULL,
  `slug` varchar(255) DEFAULT NULL,
  `name_vi_show` text,
  `created_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `display_order` tinyint(1) unsigned NOT NULL DEFAULT '1',
  `file_name` varchar(350) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `category_id` (`category_id`),
  KEY `status` (`status`),
  KEY `type` (`type`),
  KEY `category_id_2` (`category_id_2`),
  KEY `display_order` (`display_order`),
  KEY `featured` (`featured`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Table structure for table `gas_murad_product_image`
--

CREATE TABLE IF NOT EXISTS `gas_murad_product_image` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `type` tinyint(1) unsigned DEFAULT NULL,
  `product_id` mediumint(11) unsigned DEFAULT NULL,
  `file_name` text,
  `order_number` tinyint(1) unsigned DEFAULT NULL COMMENT 'số thứ tự',
  `created_date` date DEFAULT NULL COMMENT 'sẽ lấy theo ngày tạo của root record, vì liên quan đến phần lưu và xóa file',
  PRIMARY KEY (`id`),
  KEY `ticket_id` (`type`),
  KEY `order_number` (`order_number`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Table structure for table `gas_murad_product_info`
--

CREATE TABLE IF NOT EXISTS `gas_murad_product_info` (
  `id` mediumint(6) unsigned NOT NULL AUTO_INCREMENT,
  `product_id` mediumint(5) unsigned DEFAULT NULL,
  `short_description` text,
  `description` text,
  `info` text,
  `how_to_use` text,
  `component` text NOT NULL COMMENT 'Thanh phan',
  `url_video` text NOT NULL,
  PRIMARY KEY (`id`),
  KEY `category_id` (`product_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Table structure for table `gas_murad_video`
--

CREATE TABLE IF NOT EXISTS `gas_murad_video` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `category_id` smallint(3) unsigned DEFAULT NULL,
  `type` tinyint(1) unsigned DEFAULT NULL COMMENT '1: video, 2: audio',
  `name` text,
  `status` tinyint(1) unsigned DEFAULT NULL,
