CREATE TABLE IF NOT EXISTS `gas_murad_product` (
  `id` mediumint(6) unsigned NOT NULL AUTO_INCREMENT,
  `code_real` varchar(100) DEFAULT NULL,
  `category_id` smallint(3) unsigned DEFAULT NULL COMMENT 'primary category',
  `category_type` tinyint(1) DEFAULT NULL COMMENT 'category type: Mụn, Nám & lão hóa, Cellulite, Vấn đề da khác',
  `category_id_2` smallint(3) unsigned DEFAULT NULL COMMENT 'category 2 cua product',
  `featured` tinyint(1) NOT NULL DEFAULT '0',
  `name` varchar(350) DEFAULT NULL,
  `status` tinyint(1) unsigned DEFAULT NULL,
  `type` tinyint(1) unsigned DEFAULT NULL COMMENT '1: Sữa rửa mặt, 2: Điều trị, 3: Kem dưỡng, chống nắng',
  `price_retail` decimal(16,2) DEFAULT NULL,
  `price_discount` decimal(10,0) NOT NULL,
  `unit` varchar(50) DEFAULT NULL COMMENT 'đơn vị:chai, lọ, cái',
  `unit_use` varchar(50) DEFAULT NULL COMMENT 'đơn vị sử dụng: l, ml, gram...',
  `size` varchar(50) DEFAULT NULL COMMENT '1 chai bao nhiêu ml:vd 200ml',
  `size_en` varchar(100) DEFAULT NULL COMMENT 'size nuoc ngoai',
  `name_vi` varchar(350) DEFAULT NULL,
  `slug` varchar(450) DEFAULT NULL,
  `name_vi_show` varchar(350) DEFAULT NULL,
  `created_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `display_order` tinyint(1) unsigned NOT NULL DEFAULT '1',
  `file_name` varchar(350) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `category_id` (`category_id`),
  KEY `status` (`status`),
  KEY `type` (`type`),
  KEY `category_id_2` (`category_id_2`),
  KEY `display_order` (`display_order`),
  KEY `featured` (`featured`)
) ENGINE=InnoDB  DEFAULT CHARSET=utf8 AUTO_INCREMENT=65 ;

